/*
 * standaardprogramma voor de ultrasone afstandsensor
 */

task main()
{
  SetSensorLowspeed (S4);     //start sensor
  OnFwd (OUT_AC,50);          //rij vooruit
  while(SensorUS(S4) > 20);   //zolang de afstand kleiner is dan 20cm
  {
   OnRev(OUT_AC, 50);         //rij achteruit
  }
}
